{
    "name": "debranova/contact",
    "type": "library",
    "description": "General code for Debranova contacts",
    "keywords": ["project"],
    "homepage": "http://debranova.org",
    "authors": [
        {
            "name": "Johan van der Heide",
            "email": "johan.van.der.heide@itea2.org"
        },
        {
            "name": "Andre Hebben",
            "email": "andre.hebben@artemis-ia.eu"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "debranova/general",
                "source": {
                    "type": "git",
                    "url": "git@github.com:debranova/general.git",
                    "reference": "master"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "zendframework/zendframework": "2.2.*",
        "debranova/general": "master",
        "bjyoungblood/bjy-authorize": "dev-master",
        "doctrine/doctrine-orm-module": "dev-master",
        "gedmo/doctrine-extensions": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpmd/phpmd": "1.4.*",
        "phploc/phploc": "*"
    },
    "autoload": {
        "psr-0": {
            "Contact\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
