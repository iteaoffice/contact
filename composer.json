{
  "name": "iteaoffice/contact",
  "type": "library",
  "license": "proprietary",
  "description": "Code to manage contact",
  "keywords": [
    "contact"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "zendframework/zendframework": "~3.0"
  },
  "require-dev": {
    "iteaoffice/testing": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Contact\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ContactTest\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev",
      "dev-develop": "3.1.x-dev"
    }
  }
