{
  "name": "iteaoffice/contact",
  "type": "library",
  "license": "proprietary",
  "description": "Code to manage contact",
  "keywords": [
    "contact"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2",
    "zendframework/zend-mvc": "^3.0.1",
    "zendframework/zend-mvc-form": "^1.0.0",
    "zendframework/zend-code": "@stable",
    "zendframework/zend-mvc-console": "^1.2.0",
    "zendframework/zend-mvc-plugins": "^1.0.1",
    "zendframework/zend-navigation": "^2.9.0",
    "kokspflanze/bjy-authorize": "^1.6.2",
    "doctrine/doctrine-orm-module": "@stable",
    "mailjet/mailjet-apiv3-php": "1.2.*"
  },
  "require-dev": {
    "iteaoffice/testing": "^1.1.4",
    "iteaoffice/general": "^3.0",
    "iteaoffice/affiliation": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Contact\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ContactTest\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev",
      "dev-develop": "3.1.x-dev"
    }
  }
}
