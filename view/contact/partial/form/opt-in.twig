{% do form.prepare() %}
{{ form().openTag(form)|raw }}

{% set optin = form.get('contact_entity_optin') %}
{{ lbs5formelement(optin.get('id')) }}
{{ lbs5formelement(optin.get('optIn')) }}
{{ lbs5formelement(optin.get('active')) }}
{{ lbs5formelement(optin.get('description')) }}

{{ lbs5formelement(form.get('csrf')) }}

<div class="row mb-3">
    <div class="offset-sm-3 col-sm-9">
        {{ lbs5formelement(form.get('submit')) }}
        {{ lbs5formelement(form.get('cancel')) }}
        {% if form.has('delete') %}
            {{ lbs5formelement(form.get('delete')) }}
        {% endif %}
    </div>
</div>

{{ form().closeTag()|raw }}
